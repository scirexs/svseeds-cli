{
  "name": "@svseeds/cli",
  "version": "0.1.0",
  "license": "MIT",
  "exports": "./mod.ts",
  "publish": {
    "include": [
      "mod.ts",
      "src/**/*.ts",
      "README.md",
      "LICENSE"
    ]
  },
  "nodeModulesDir": "auto",
  "lint": {
    "include": ["src/", "tests/"],
    "rules": {
